{
"extends": "airbnb",
"env": {
  "browser": true,
  "node": true,
  "jquery": true
  },
  "plugins": ["html", "json"],
  "rules": {
    "no-underscore-dangle": "off",
    "func-names": "off",
    "class-methods-use-this": "off",
    "no-use-before-define": "off",
    "no-unused-vars": "warn",
    "comma-dangle": "warn",
    "global-require": "warn",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true, "optionalDependencies": false, "peerDependencies": false}],
    "import/first": false,
    }
}
